{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "esmodules": true
            },
            "bugfixes": true,
            "modules": false,
            "useBuiltIns": false
        }],
        ["@babel/preset-typescript", {
            "optimizeConstEnums": true
        }],
        ["@babel/preset-react", {
            "runtime": "automatic"
        }]
    ]
}
